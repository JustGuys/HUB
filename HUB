    PremiumOnly = false
   })
   
   local Tab = Window:MakeTab({
	Name = "Aimbot",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
   
Tab:AddButton({
	Name = "Aimbot",
	Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/JustGuys/ArsenalScript/main/Aimbot"))();
  	end    
})



